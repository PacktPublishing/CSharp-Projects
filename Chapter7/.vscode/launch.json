{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Uno Platform Mobile Debug",
      "type": "Uno",
      "request": "launch",
      "preLaunchTask": "Uno: android | Debug | android-x64"
    },
    {
      "name": "Uno Platform WebAssembly Debug (Chrome)",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:5000",
      "webRoot": "${workspaceFolder}/Chatbot",
      "inspectUri": "{wsProtocol}://{url.hostname}:{url.port}/_framework/debug/ws-proxy?browser={browserInspectUri}",
      "timeout": 30000,
      "preLaunchTask": "build-wasm",
      "server": {
        "runtimeExecutable": "dotnet",
        "program": "run",
        "args": ["--no-build", "-f", "net9.0-browserwasm", "--launch-profile", "Chatbot (WebAssembly)"],
        "outputCapture": "std",
        "timeout": 30000,
        "cwd": "${workspaceFolder}/Chatbot"
      }
    },
    {
      "name": "Uno Platform WebAssembly Debug (Edge)",
      "type": "msedge",
      "request": "launch",
      "url": "http://localhost:5000",
      "webRoot": "${workspaceFolder}/Chatbot",
      "inspectUri": "{wsProtocol}://{url.hostname}:{url.port}/_framework/debug/ws-proxy?browser={browserInspectUri}",
      "timeout": 30000,
      "preLaunchTask": "build-wasm",
      "server": {
        "runtimeExecutable": "dotnet",
        "program": "run",
        "args": ["--no-build", "-f", "net9.0-browserwasm", "--launch-profile", "Chatbot (WebAssembly)"],
        "outputCapture": "std",
        "timeout": 30000,
        "cwd": "${workspaceFolder}/Chatbot"
      }
    },
    {
      "name": "Uno Platform Desktop Debug",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-desktop",
      "program": "${workspaceFolder}/Chatbot/bin/Debug/net9.0-desktop/Chatbot.dll",
      "args": [],
      "launchSettingsProfile": "Chatbot (Desktop)",
      "env": {
        "DOTNET_MODIFIABLE_ASSEMBLIES": "debug"
      },
      "cwd": "${workspaceFolder}/Chatbot",
      "console": "internalConsole",
      "stopAtEntry": false
    }
  ]
}
