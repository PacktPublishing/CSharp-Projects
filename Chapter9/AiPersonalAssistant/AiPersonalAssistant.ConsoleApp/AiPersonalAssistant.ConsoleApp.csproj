<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="appsettings.json" />
    <None Remove="Data\announcing-data-science-in-dotnet.md" />
    <None Remove="Data\CopyrightNote.md" />
    <None Remove="Data\ml_net-video-game-classification.md" />
    <None Remove="Data\ml_net_2_0_text_classification.md" />
    <None Remove="Data\penalty-prediction.md" />
    <None Remove="Data\reinforcement_learning.md" />
